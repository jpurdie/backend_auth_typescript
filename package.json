{
  "name": "ppm_backend",
  "version": "1.4.1",
  "description": "PPM Backend.",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node src/server.ts",
    "build-ts": "tsc",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "tslint": "npm run tslint-fix && tslint -c tslint.json -p tsconfig.json",
    "tslint-fix": "tslint --fix -c tslint.json -p tsconfig.json",
    "serve": "node dist/server.js",
    "build": "npm run tslint && npm run build-ts && npm run copy-static-assets",
    "start": "npm run serve",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.x.x"
  },
  "author": "Javier Aviles",
  "license": "MIT",
  "homepage": "https://frontend-justindpurdie242881.codeanyapp.com",
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/node": "^10.17.9",
    "@types/shelljs": "^0.8.6",
    "nodemon": "^1.19.4",
    "shelljs": "^0.8.3",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "auth0": "^2.20.0",
    "axios": "^0.19.1",
    "bcrypt": "^3.0.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-validator": "^0.9.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^2.3.1",
    "express-validator": "^6.3.0",
    "helmet": "^3.21.2",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "mailgun-js": "^0.22.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.14.0",
    "pg-connection-string": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^2.3.0",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.21",
    "winston": "^3.2.1"
  }
